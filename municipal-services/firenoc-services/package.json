{
  "name": "egov-firenoc-services",
  "version": "1.3.2_beta",
  "private": true,
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEBUG=swagger-express-validator nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "cross-env NODE_ENV=production node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "prod:start": "cross-env NODE_ENV=production node dist"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.0",
    "ajv": "^6.10.0",
    "axios": "^0.18.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "cross-env": "^5.2.0",
    "express": "^4.13.3",
    "express-async-handler": "^1.1.4",
    "kafka-node": "^4.1.3",
    "morgan": "^1.8.0",
    "pg": "^7.6.1",
    "random-number": "0.0.9",
    "resource-router-middleware": "^0.6.0",
    "swagger-express-validator": "^1.0.0",
    "swagger-model-validator": "^3.0.10",
    "swagger-ui-express": "^4.0.2",
    "util": "^0.12.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
